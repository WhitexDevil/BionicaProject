<UserControl x:Class="project.Controls.GaControlBasic"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
              xmlns:local="clr-namespace:project.Controls"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <local:UserControlInt x:Name="Population" yProp="Population size"  HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
        <local:UserControlInt x:Name="Generation" yProp="Generation size" HorizontalAlignment="Left" Margin="10,80,0,0" VerticalAlignment="Top"/>
        <local:UserControlInt x:Name="Battles" yProp="Battles count" HorizontalAlignment="Left" Margin="10,150,0,0" VerticalAlignment="Top"/>
        <local:UserControlInt x:Name="WintRate" yProp="Goal Wint rate(in %): "  HorizontalAlignment="Left" Margin="10,220,0,0" VerticalAlignment="Top"/>
        <Label Content="GA Parametres"  Margin="106,0,104,0" VerticalAlignment="Top"/>
    </Grid>
</UserControl>
