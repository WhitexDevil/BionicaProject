<UserControl x:Class="project.Controls.LabelControl1"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="81.203" d:DesignWidth="300">
    <Grid Margin="0,0,0,10">
        <Slider  Name="Slider1" HorizontalAlignment="Left" Margin="10,39,0,0" VerticalAlignment="Top" Width="280"/>
        <Label Name="LabelSlider1" Content="{Binding ElementName=Slider1, Path=Value}"  HorizontalAlignment="Left" Margin="68,10,0,0" VerticalAlignment="Top" Height="29" Width="104"/>
        <Label Content="" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10,10,0,0" Height="24" Width="53"/>

    </Grid>
</UserControl>
